      "const numDeal = function(num) {",
      "  if (!num) return;",
      "  let reg = /\\d{1,3}(?=(\\d{3})+$)/g;",
      "  let numArr = num.toString().split(\".\");",
      "  let point = numArr[1] ? \".\" + numArr[1] : \"\";",
      "  return numArr[0].replace(reg, \"$&,\") + point;",
      "};"
